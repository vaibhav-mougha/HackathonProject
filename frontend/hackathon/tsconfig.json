{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/Components/Register.tsx", "src/pages/register/AuthContext.jsx", "src/pages/register/register.tsx", "src/pages/login.jsx", "src/pages/_app.jsx", "src/Redux/action.js", "src/Redux/actiontypes.js", "src/Redux/reducer.js", "src/Redux/store.js", "src/pages/signup/index.jsx", "src/pages/login/index.jsx"],
  "exclude": ["node_modules"]
}
